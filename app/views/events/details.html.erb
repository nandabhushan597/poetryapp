<h1><%= @event_info ["Name"] %></h1> 
<p> Details: <%= @event_info["Description"] %></p>
<%= simple_form_for([current_user, current_user.events.build]) do |f| %>
	<%= f.hidden_field :title, :value => @event_info["Title"] %>
	<%= f.hidden_field :year, :value => @event_info["Year"] %> 
<% end %> 
